{
  "name": "jenkins-dashboard",
  "version": "1.0.0",
  "description": "jenkins dashboard",
  "scripts": {
    "test": "jsxhint lib/ spec/ && jshint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "webpack -p",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "dev": "webpack-dev-server --inline --hot",
    "postinstall": "bower install"
  },
  "author": "",
  "license": "BSD",
  "devDependencies": {
    "babel-core": "~5.4.3",
    "babel-loader": "~5.1.2",
    "css-loader": "~0.12.1",
    "es5-shim": "~4.1.0",
    "jasmine": "~2.2.1",
    "jasmine-core": "~2.2.0",
    "jshint": "~2.6.0",
    "jsxhint": "~0.15.0",
    "karma": "~0.12.31",
    "karma-jasmine": "~0.3.5",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-webpack": "~1.5.0",
    "sass-loader": "~1.0.2",
    "style-loader": "~0.12.2",
    "webpack": "~1.5.3",
    "webpack-dev-server": "~1.7.0"
  },
  "dependencies": {
    "react": "~0.12.2"
  }
}
